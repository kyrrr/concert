{# app/Resources/views/layout.html.twig #}
<!doctype html>
<html>
    <head>
        <script type="text/javascript"
                src="{{ asset('assets/vendor/jquery/dist/jquery.min.js') }}">
        </script>
        <link rel="stylesheet"
            href="{{ asset('assets/vendor/bootstrap/dist/css/bootstrap.min.css') }}">
        <link rel="stylesheet"
            href="{{ asset('assets/custom/css/grid.css') }}">
        <link rel="stylesheet"
            href="{{ asset('assets/custom/css/footer.css') }}">
        <link rel="stylesheet"
            href="{{ asset('assets/custom/less/select.css') }}">
        

        <script type="text/javascript" src="{{ asset('assets/custom/js/events.js') }}"></script>
    </head>
    <body>
    	<div class="container">

      <div class="page-header">
        <h1>Symfony ♡ Bootstrap ♡ Kyrr </h1>
        <p class="lead">&#9904; Pretty page for testing things &#9904;</p>
      </div>
     

      <h3>{{ myVar }}</h3>
      <p>Tekst her <strong>som understreker noe</strong>. Kul og responsiv.</p>
      <div class="row">
      {% for value in myArray %}
        <div class="col-md-4"> {{ value }}</div>
      {% endfor %}
      </div>

      
    </div> <!-- /container -->

    <footer class="footer">
      <div class="container">
        <p class="text-muted">Kyrro Industries 2016</p>
      </div>
    </footer>

    </body>
</html>